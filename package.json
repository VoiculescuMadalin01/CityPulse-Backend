        "@nestjs/swagger": "^7.4.0",
        "@prisma/client": "^5.17.0",
        "@supabase/supabase-js": "^2.45.0",
        "@types/multer": "^1.4.11",
        "bcrypt": "^5.1.1",
        "class-transformer": "^0.5.1",
        "class-validator": "^0.14.1",
        "csv-parser": "^3.0.0",
        "dotenv": "^16.4.5",
        "express": "^4.19.2",
        "passport": "^0.7.0",
